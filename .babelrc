{
  "presets": [
    "env",
    "react",
    "stage-0",
    "es2016"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-decorators",
    "transform-react-constant-elements",
    "transform-react-inline-elements",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ]
}
